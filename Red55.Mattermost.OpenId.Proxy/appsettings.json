{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ReverseProxy": {
    "Clusters": {
      "gitlab": {
        "Destinations": {
          "gitlab": {
            "Address": "http://gitlab",
          }
        }
      }
    },

    "Routes": {
      "gitlab": {
        "ClusterId": "gitlab",
        "Match": {
          "Path": "{**catch-all}"
        },
        "Transforms": [ { "RequestHeadersCopy": "false" } ]
      }
    }
  }
}
